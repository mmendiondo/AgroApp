<%- model_class = Establecimiento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>


<div class="row-fluid" >
  <div class="col-md-12" >
    <%=render 'establecimientos_grid' %>

    <%= link_to t('.new', :default => t("helpers.links.new")),
                new_establecimiento_path,
                :class => 'btn btn-primary' %>

    <button class="btn btn-primary wg-external-csv-export-button" data-grid-name="establecimiento">
